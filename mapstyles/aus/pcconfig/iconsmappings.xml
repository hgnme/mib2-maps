<?xml version="1.0" encoding="utf-8"?>
<iconsmappings>
  <!-- If no resolution is found by the PresCon in this config, default value 10 will be used -->
  <resolutions>
    <!--VW default-->
    <res width="800" height="480" sdk_icon_initial_size="10" />
    <!--VW GEN2-->
    <res width="1280" height="640" sdk_icon_initial_size="30" />
  </resolutions>
  <!-- Mapping between dsi icon sizes and sdk icon sizes -->
  <!-- If no coding bit is found by the PresCon in this config, default values will be used:
         dsi_value="0" sdk_icon_size="10" internal_icon_size="0"
         dsi_value="1" sdk_icon_size="20" internal_icon_size="1"
         dsi_value="2" sdk_icon_size="30" internal_icon_size="2"
         dsi_value="3" sdk_icon_size="20" internal_icon_size="1"
         dsi_value="4" sdk_icon_size="20" internal_icon_size="1" -->
  <dsi>
    <coding bit="0">
      <!-- Mapping for DSI value: ICONSIZE_SMALL = 0 -->
      <mapping dsi_value="0" sdk_icon_size="10" internal_icon_size="0" />
      <!-- Mapping for DSI value: ICONSIZE_MEDIUM = 1 -->
      <mapping dsi_value="1" sdk_icon_size="20" internal_icon_size="1" />
      <!-- Mapping for DSI value: ICONSIZE_LARGE = 2 -->
      <mapping dsi_value="2" sdk_icon_size="30" internal_icon_size="2" />
      <!-- Mapping for DSI value: ICONSIZE_VERY_SMALL = 3 -->
      <mapping dsi_value="3" sdk_icon_size="20" internal_icon_size="1" />
      <!-- Mapping for DSI value: ICONSIZE_VERY_LARGE = 4 -->
      <mapping dsi_value="4" sdk_icon_size="20" internal_icon_size="1" />
    </coding>
    <coding bit="1">
      <!-- Mapping for DSI value: ICONSIZE_SMALL = 0 -->
      <mapping dsi_value="0" sdk_icon_size="30" internal_icon_size="2" />
      <!-- Mapping for DSI value: ICONSIZE_MEDIUM = 1 -->
      <mapping dsi_value="1" sdk_icon_size="40" internal_icon_size="2" />
      <!-- Mapping for DSI value: ICONSIZE_LARGE = 2 -->
      <mapping dsi_value="2" sdk_icon_size="40" internal_icon_size="3" />
      <!-- Mapping for DSI value: ICONSIZE_VERY_SMALL = 3 -->
      <mapping dsi_value="3" sdk_icon_size="40" internal_icon_size="3" />
      <!-- Mapping for DSI value: ICONSIZE_VERY_LARGE = 4 -->
      <mapping dsi_value="4" sdk_icon_size="40" internal_icon_size="3" />
    </coding>
  </dsi>
</iconsmappings>